  delay(100);
  tft.fillRect(64,0,34,128,ST7735_YELLOW);
  delay(100);
  tft.fillRect(96,0,34,128,ST7735_GREEN);
  delay(100);
  tft.fillRect(128,0,34,128,ST7735_CYAN);
  delay(200);

  
  tft.setFont(&Mokoto_Glitch6pt7b);

  tft.setTextWrap(true); 

  tft.setCursor(35,20);
 
  tft.setTextColor(ST7735_WHITE);
  
  
  tft.print("P");
  delay(100);
  tft.print("u");
  delay(100);
  tft.print("l");
  delay(100);
  tft.print("p");
  delay(100);
